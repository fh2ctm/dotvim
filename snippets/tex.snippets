snippet e "Environment"
	\\begin{$1}
	$0
	\\end{$1}

snippet a "Align"
	\\begin{align*}
		$0
	\\end{align*}

snippet i "Inline"
	\$$1\$

snippet d "Display"
	\$\$
	$0
	\$\$

snippet mb "Math Blackboard"
	\\mathbb{$1}

snippet sn "Section no-numbering"
	\\section*{$1}

snippet ssn "Subsection no-numbering"
	\\subsection*{$1}

snippet n "Newline"
	\\newline

snippet p "Paragraph"
	\\paragraph{$1}
	
snippet bf "Boldface"
	\\textbf{$1}
